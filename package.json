{
  "name": "next-js-template",
  "version": "0.0.1",
  "description": "A template for next-js apps",
  "main": "index.js",
  "author": "Your Name<email@example.com>",
  "license": "MIT",
  "keywords": [
    "next",
    "react",
    "redux",
    "typescript",
    "emotion"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development next",
    "build": "cross-env NODE_ENV=production next build",
